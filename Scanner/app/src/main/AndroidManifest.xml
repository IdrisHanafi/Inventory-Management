<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.squad.scanner" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Scan"
            android:label="@string/title_activity_scan" >
        </activity>
        <activity
            android:name=".ManagerScreen"
            android:label="@string/title_activity_manager_screen" >
        </activity>
        <activity
            android:name=".NormalUserScreen"
            android:label="@string/title_activity_normal_user_screen" >
        </activity>
        <activity
            android:name=".RegisterUserScreen"
            android:label="@string/title_activity_register_user_screen" >
        </activity>
        <activity
            android:name=".CreateProductScreen"
            android:label="@string/title_activity_create_product_screen" >
        </activity>
        <activity
            android:name=".CostAnalysisScreen"
            android:label="@string/title_activity_cost_analysis_screen" >
        </activity>
    </application>

</manifest>
